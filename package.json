  "bugs": {
    "url": "https://github.com/ntutchton/npm-ts-lib/issues"
  },
  "homepage": "https://github.com/ntutchton/npm-ts-lib#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
